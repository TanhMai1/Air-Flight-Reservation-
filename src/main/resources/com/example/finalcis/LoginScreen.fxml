<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalcis.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="My Account" textAlignment="CENTER" wrappingWidth="161.375">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Button mnemonicParsing="false" text="My Flights" />
       <Button mnemonicParsing="false" onAction="#handleBookFlightButton" text="Book Flight" />
       <Button mnemonicParsing="false" text="Delete a flight" />
      <Button mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
   </children>
</VBox>
