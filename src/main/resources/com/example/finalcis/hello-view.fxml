    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="Login" onAction="#onLoginClick" prefHeight="25.0" prefWidth="69.0" text="Login" />
    <Label fx:id="LoginLabel" />

    <Button fx:id="Register" onAction="#onLRegisterClick" prefHeight="25.0" prefWidth="72.0" text="Register" />
    <Label fx:id="RegisterLabel" />
</VBox>
